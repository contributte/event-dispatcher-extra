{
  "name": "contributte/event-dispatcher-extra",
  "description": "Nette-based Symfony events for application, presenter, form, latte, templates, security and many others.",
  "keywords": [
    "nette",
    "event",
    "dispatcher",
    "bridge",
    "application",
    "security",
    "form",
    "latte",
    "template"
  ],
  "type": "library",
  "license": "MIT",
  "homepage": "https://github.com/contributte/event-dispatcher-extra",
  "authors": [
    {
      "name": "Milan Felix Å ulc",
      "homepage": "https://f3l1x.io"
    }
  ],
  "require": {
    "php": ">=7.2",
    "nette/di": "~3.0.0",
    "symfony/event-dispatcher": "^4.3.1|^5.0.0"
  },
  "require-dev": {
    "contributte/event-dispatcher": "^0.8.0",
    "latte/latte": "^2.5.1",
    "nette/application": "~3.0.0",
    "nette/http": "~3.0.1",
    "nette/security": "~3.0.0",
    "ninjify/nunjuck": "^0.4.0",
    "ninjify/qa": "^0.12.0",
    "phpstan/extension-installer": "^1.0",
    "phpstan/phpstan-deprecation-rules": "^1.0",
    "phpstan/phpstan-nette": "^1.0",
    "phpstan/phpstan": "^1.0",
    "phpstan/phpstan-strict-rules": "^1.0"
  },
  "conflict": {
    "nette/di": "<3.0.0"
  },
  "autoload": {
    "psr-4": {
      "Contributte\\Events\\Extra\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\Fixtures\\": "tests/fixtures"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "sort-packages": true
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.9.x-dev"
    }
  }
}
