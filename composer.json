{
  "name": "contributte/event-dispatcher-extra",
  "description": "Nette-based Symfony events for application, presenter, form, latte, templates, security and many others.",
  "keywords": [
    "nette",
    "event",
    "dispatcher",
    "bridge",
    "application",
    "security",
    "form",
    "latte",
    "template"
  ],
  "type": "library",
  "license": "MIT",
  "homepage": "https://github.com/contributte/event-dispatcher-extra",
  "authors": [
    {
      "name": "Milan Felix Å ulc",
      "homepage": "https://f3l1x.io"
    }
  ],
  "require": {
    "php": ">=7.4",
    "nette/di": "^3.1.2",
    "symfony/event-dispatcher": "^5.0 || ^6.0"
  },
  "require-dev": {
    "contributte/event-dispatcher": "^0.8.0",
    "latte/latte": "^2.11.6",
    "nette/application": "^3.1.10",
    "nette/http": "^3.2.1",
    "nette/security": "^3.1.7",
    "ninjify/nunjuck": "^0.4.0",
    "ninjify/qa": "^0.14.0",
    "phpstan/phpstan-deprecation-rules": "^1.1.2",
    "phpstan/phpstan-nette": "^1.2.4",
    "phpstan/phpstan": "^1.10.6",
    "phpstan/phpstan-strict-rules": "^1.5.0"
  },
  "autoload": {
    "psr-4": {
      "Contributte\\Events\\Extra\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.9.x-dev"
    }
  }
}
