{
  "name": "contributte/event-dispatcher-extra",
  "description": "Nette-based Symfony events for application, presenter, form, latte, templates, security and many others.",
  "keywords": [
    "nette",
    "event",
    "dispatcher",
    "bridge",
    "application",
    "security",
    "form",
    "latte",
    "template"
  ],
  "type": "library",
  "license": "MIT",
  "homepage": "https://github.com/contributte/event-dispatcher-extra",
  "authors": [
    {
      "name": "Milan Felix Å ulc",
      "homepage": "https://f3l1x.io"
    }
  ],
  "require": {
    "php": ">= 7.1",
    "nette/di": "~2.4.12 || ~3.0.0",
    "nette/utils": "~2.5.2 || ~3.0.0"
  },
  "require-dev": {
    "contributte/event-dispatcher": "^0.5.0",
    "latte/latte": "~2.4.7",
    "nette/application": "~2.4.12",
    "nette/http": "~2.4.9",
    "nette/security": "~2.4.3",
    "ninjify/nunjuck": "^0.2.0",
    "ninjify/qa": "^0.8.0",
    "phpstan/extension-installer": "^1.0",
    "phpstan/phpstan-deprecation-rules": "^0.11",
    "phpstan/phpstan-nette": "^0.11",
    "phpstan/phpstan-shim": "^0.11",
    "phpstan/phpstan-strict-rules": "^0.11"
  },
  "conflict": {
    "latte/latte": "<2.4.7",
    "nette/application": "<2.4.12",
    "nette/security": "<2.4.3",
    "nette/http": "<2.4.9",
    "contributte/event-dispatcher": "<0.3.1"
  },
  "autoload": {
    "psr-4": {
      "Contributte\\Events\\Extra\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\Fixtures\\": "tests/fixtures"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "sort-packages": true
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.5.x-dev"
    }
  }
}
